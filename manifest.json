{
    "manifest_version": 2,
    "name": "Privacy Search",
    "description": "__MSG_extDescription__",
    "author":"David Legrand",
    "version": "0.1.0.5",
    "default_locale": "en",
    "icons": {
        "16": "ico/privacy-16.png",
        "24": "ico/privacy-24.png",
        "32": "ico/privacy-32.png",
        "64": "ico/privacy-64.png",
        "128": "ico/privacy-128.png",
        "256": "ico/privacy-256.png",
        "512": "ico/privacy-512.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent":true
    },
    "options_ui": {
        "page": "html/options.html",
        "open_in_tab": false
    },
    "homepage_url": "http://www.nextinpact.com"
}